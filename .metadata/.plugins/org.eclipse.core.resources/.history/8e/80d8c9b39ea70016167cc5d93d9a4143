/**
 * @note Public functions
 */

int32_t appInitOutputs(){
	return 0;
}

int32_t appInitADC(){
	//Inicializo el ADC
	adcInit(ADC_FREQUENCY, appADCEndOfConversionHandler);
	//Cargo un valor
	adcConfigChannel((adcEChannel) 0);
	//Arranco las mediciones
	adcStart();

	return 0;
}
